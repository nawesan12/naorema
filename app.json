{
  "expo": {
    "name": "Naorema",
    "slug": "naorema",
    "version": "1.2.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#214252"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#214252"
      },
      "package": "com.naorema.app"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
